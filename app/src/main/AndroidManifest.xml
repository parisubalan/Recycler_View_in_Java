<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.parisubalan.recyclerview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Recycler_View"
        android:fullBackupContent="">
        <activity
            android:name="com.parisubalan.recyclerview.activity.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.parisubalan.recyclerview.activity.RecyclerActivity">
        </activity>

        <activity android:name="com.parisubalan.recyclerview.activity.StaggeredGridActivity">
        </activity>

        <activity android:name="com.parisubalan.recyclerview.activity.GridActivity">
        </activity>

    </application>

</manifest>